{% extends "::base.html.twig" %}

{% block content %}

    <div class="container">
        <div class="row">
            <div class="col s12">
                <h1>Students list</h1>

                <ul>
                    {% for student in students %}
                        <li>{{ student.firstName }} - {{ student.lastName }}
                            - <a href="{{ path('student_delete', {id: student.id}) }}">Delete</a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>

{% endblock %}